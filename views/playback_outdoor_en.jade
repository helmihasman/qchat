extends base_playback
block content 
    .right_col(role='main', style='background-color:#49A8FF')
        .row
          .col-md-12
                .f_panel
                  .f_title
                    .col-xs-12
                        ul.nav.nav-tabs.bar_tabs
                          li
                            a(href='/tracking') Real Time Tracking
                          
                          li.active
                            a(href='/path_playback_outdoor') Outdoor Playback
                          li
                            a(href='/path_playback') Indoor Playback
                          li
                            a(href='/geofencing') Geofencing
                    .clearfix
                  .f_content
                    
                    div(role='tabpanel', data-example-id='togglable-tabs')
                          .row
                            // form input mask
                            .col-md-2.col-sm-12.col-xs-12
                                #tree_outdoor
                                  


                            .col-md-10.col-sm-12.col-xs-12
                                    input(type='text', name='map_string_load',id='map_string_load',hidden,value='#{maps[0].map_string}')
                                    input(type='text', name='map_string',id='map_string',hidden,value='#{maps[0].map_string}')
                                    #panel-content
                                    form.form-horizontal
                                    table
                                        tr
                                            td
                                                label &nbsp;Start Date:&nbsp;
                                            td
                                                #datetimepicker1.input-group.date.col-md-12
                                                    input.form-control(type='text',id='time1')
                                                    span.input-group-addon
                                                      span.glyphicon.glyphicon-calendar
                                        
                                            td
                                                label &nbsp;&nbsp;End Date:&nbsp;
                                            td
                                                #datetimepicker2.input-group.date.col-md-12
                                                  input.form-control(type='text',id='time2')
                                                  span.input-group-addon
                                                    span.glyphicon.glyphicon-calendar
                                                
                                            td  &nbsp;&nbsp;&nbsp;&nbsp;
                                                button.btn.btn-primary(type='button',onclick='playbackOutdoor()') Submit
                                            td
                                                button.btn.btn-info#playButton.fa.fa-play
                                                

                                        #controls(hidden)
                                          select#polyList
                                          button#btnDelete Delete
                                          button(type='button',onclick="refresh();") Save
                                    #googleMapPlayback(style='width:100%;height:700px;')
                                    
                          .row
                              .col-md-12
                                // form date pickers
                                .x_panel
                                  .x_title
                                    h2 Timeline
                                    ul.nav.navbar-right.panel_toolbox
                                    .clearfix
                                  .x_content(style='overflow: auto')
                                    p
                                      #wizard.form_wizard.wizard_horizontal
                                        ul#wizard_steps.wizard_steps
                                         
                                        

                                    //.well
                                      form.form-horizontal
                                        fieldset
                                          .control-group(style='width: 264px')
                                            label#Label1(style='width: 40px') Date :
                                            input(type='date')
                                          .x_content
                                            .row.grid_slider
                                              .col-md-12.col-sm-6.col-xs-12
                                                input#range_31(type='text', value='', name='range')
// /page content


